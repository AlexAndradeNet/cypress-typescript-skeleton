{
    "name": "cypress-typescript-skeleton",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test-ui": "npx cypress open",
        "test": "npx cypress run",
        "sonar-unix": "node scripts/sonar-project.js",
        "sonar-windows": "node scripts\\sonar-project.js",
        "lintFix:feature": "npx gherkin-lint --ignore .gitignore --config .gherkin-lintrc.json",
        "lintFix:json": "npx prettier --ignore-path .gitignore '**/*.json' --list-different --write",
        "lintFix:js": "npx eslint --ignore-path .gitignore --fix .",
        "lint": "npm run lintFix:feature ; npm run lintFix:json ; npm run lintFix:js",
        "lint-ci": "npm run lintFix:feature && npm run lintFix:json && npm run lintFix:js",
        "prepare": "husky",
        "postinstall": "husky",
        "pre-commit": "npm run lint-ci",
        "pre-push": "npm run pre-commit && npm run test"
    },
    "keywords": [],
    "author": "Alex Andrade",
    "license": "ISC",
    "dependencies": {
        "cypress": "^13.14.1",
        "cypress-iframe": "^1.0.1",
        "@testing-library/cypress": "^10.0.2",
        "@badeball/cypress-cucumber-preprocessor": "^20.1.2",
        "@bahmutov/cypress-esbuild-preprocessor": "^2.2.2"
    },
    "devDependencies": {
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^22.5.0",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-cypress": "^3.5.0",
        "eslint-plugin-prettier": "^5.2.1",
        "gherkin-lint": "^4.2.4",
        "husky": "^9.1.5",
        "prettier": "^3.3.3",
        "sonarqube-scanner": "^4.2.1",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.2.0"
    }
}
